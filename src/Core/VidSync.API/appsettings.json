{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Debug",
      "Microsoft.AspNetCore.Http.Connections": "Debug"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=HOST;Port=YOUR_PORT;Database=YOUR_DB_NAME;Username=YOUR_USERNAME;Password=YOUR_PASSWORD",
    "Redis": "localhost:6379"
  },
  "JWT": {
    "Key": "YOUR_SECRET_KEY",
    "Issuer": "YOUR_ISSUER",
    "Audience": "YOUR_AUDIENCE",
    "DurationInMinutes": 60
  },
  "SMTP": {
    "Host": "YOUR_SMTP_HOST",
    "Port": 0,
    "NetworkCredentialMail": "YOUR_EMAIL",
    "NetworkCredentialPassword": "YOUR_EMAIL_PASSWORD",
    "SenderMail": "YOUR_SENDER_EMAIL",
    "SenderName": "YOUR_SENDER_NAME"
  }
}
